[Unit]
Description=uWSGI instance to serve notion reading list server
After=network.target

[Service]
User=sevenforward
Group=www-data
WorkingDirectory=/home/sevenforward/reading_list/server
Environment="PATH=/home/sevenforward/reading_list/server/virt/bin"
ExecStart=/home/sevenforward/reading_list/server/virt/bin/uwsgi --ini publicInteg.ini

[Install]
WantedBy=multi-user.target
